<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.whydanu.informasicovid19" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/covid20"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/covid20"
        android:supportsRtl="true"
        android:theme="@style/Theme.InformasiCovid19"
        android:usesCleartextTraffic="true"
        tools:targetApi="m">
        <activity
            android:name=".Activity.ProfilActivity"
            android:exported="false" />

        <activity
            android:name=".Activity.SplascreenActivity"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".Activity.TentangActivity"
            android:exported="false"/>
        <activity
            android:name=".Activity.item_province"
            android:exported="false" />
        <activity
            android:name=".Activity.ProvinceActivity"
            android:exported="false" />
        <activity android:name=".Activity.MainActivity" >
        </activity>
    </application>

</manifest>